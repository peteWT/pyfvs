  "metadata": {
    "variant": "OP",
    "variant_name": "ORGANON Pacific Northwest",
    "source": "FVS ORGANON variant (diagro.f), Hann et al. 2006, Zumrawi & Hann 1993",
    "description": "Diameter growth coefficients for ORGANON Pacific Northwest variant",
    "equation": "ln(DG) = B0 + B1*ln(DBH+K1) + B2*DBH^K2 + B3*ln((CR+0.2)/1.2) + B4*ln(SI-4.5) + B5*(BAL^K3/ln(DBH+K4)) + B6*sqrt(BA)",
    "base_cycle": 5,
    "units": {
      "DBH": "inches",
      "SI": "feet (King 1966, base age 50)",
      "BA": "sq ft/acre",
      "BAL": "sq ft/acre",
      "DG": "inches/5yr"
    },
    "versions": {
      "1": "SWO - Southwest Oregon",
      "2": "NWO - Northwest Oregon",
      "3": "SMC - Stand Management Cooperative",
      "4": "RAP - Red Alder Plantation"
    },
    "notes": "ORGANON predicts diameter growth directly, not DDS. Coefficients are from peer-reviewed research publications."
  },
